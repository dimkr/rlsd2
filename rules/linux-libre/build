PKG_VER="4.1"
PKG_SRCS="http://linux-libre.fsfla.org/pub/linux-libre/releases/$PKG_VER-gnu/linux-libre-$PKG_VER-gnu.tar.xz"
PKG_DESC="An operating system kernel"

get_latest() {
	wget -q -O - "https://www.kernel.org/" |
	grep "4.1" |
	tail -n 1 |
	cut -f 2 -d \- |
	cut -f 1 -d \"
}

